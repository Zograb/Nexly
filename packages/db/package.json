{
  "name": "@smart-notes/db",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "cleanup": "del-cli generated --force || exit 0",
    "prisma:generate": "zenstack generate --schema schema.zmodel",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma --name ",
    "prisma:reset": "prisma migrate reset --schema prisma/schema.prisma --force --skip-seed",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@smart-notes/eslint-config": "workspace:*",
    "@smart-notes/typescript-config": "workspace:*",
    "@zenstackhq/runtime": "2.20.1",
    "graphql-type-json": "^0.3.2",
    "prisma-graphql-type-decimal": "^3.0.1",
    "prisma-nestjs-graphql": "^21.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "del-cli": "^7.0.0",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "zenstack": "2.20.1"
  }
}
